{
    "name": "modularis/installer",
    "description": "Installer for Modularis CMS",
    "license": "MIT",
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "Modularis\\Installer\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "friendsofphp/php-cs-fixer": "^3.68",
        "phpstan/phpstan": "^2.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Modularis\\Installer\\Tests\\": "tests/"
        }
    }
}
